{
  "name": "react-stack-skeleton",
  "version": "0.0.2",
  "description": "A basic sekeleton site, for an isomorphic react web app.",
  "author": "Nick Heyn <nick@heyn.io>",
  "dependencies": {
    "async-dispatcher": "^0.1.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-device": "^0.4.2",
    "express-react-router": "^2.4.0",
    "react": "^15.3.2",
    "reactcss": "^1.0.8",
    "react-async-dispatcher": "^0.1.0",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "recompose": "^0.20.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-watch": "^2.0.2",
    "concurrently": "^2.2.0",
    "flow-bin": "^0.33.0",
    "iflow-react-router": "^1.1.17",
    "jest-cli": "^16.0.2",
    "json-loader": "^0.5.4",
    "nsp": "^2.6.1",
    "react-test-renderer": "^15.3.2",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "nsp": "nsp check",
    "jest": "jest",
    "flow": "flow check",
    "babel": "babel ./src -d ./lib",
    "webpack": "NODE_ENV=production webpack --optimize-minimize --display-modules",
    "webpack:watch": "webpack --debug --devtool inline-source-map --output-pathinfo --watch",

    "test": "npm run nsp && npm run flow && npm run jest",
    "build": "npm run babel && npm run webpack",
    "server": "node ./lib/server.js",
    "server:watch": "babel-watch ./src/server.js",

    "dev": "concurrently 'npm run webpack:watch' 'npm run server:watch'",
    "start": "npm run server"
  },
  "engines": {
    "node": "v6"
  }
}
